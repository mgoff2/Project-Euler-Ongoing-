import math

A = []
B = []
C = []
for i in range(2,500):
    a = i**2
    b = i**2
    c = i**2
    A.append(a)
    B.append(b)
    C.append(c)
       
for i in A:
    for p in B: 
        for l in C:
            if i + p == l:
                i2 = math.sqrt(i)
                p2 = math.sqrt(p)
                l2 = math.sqrt(l)
                if i2 + p2 + l2 == 1000:
                    sol = i2*p2*l2
                    print(sol)
